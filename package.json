{
  "name": "jquery-plugin-bsmodal",
  "version": "1.2.0",
  "description": "bsModal is jQuery plugin, generate bootstrap 4 modal, And crop&upload image modal.",
  "main": "dist/jquery.bsmodal.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c",
    "watch": "cross-env NODE_ENV=development rollup -c -w",
    "build": "cross-env NODE_ENV=production rollup -c",
    "compress": "uglifyjs dist/jquery.bsmodal.js -o dist/jquery.bsmodal.min.js -c -m --comments /^!/",
    "release": "npm run build && npm run compress"
  },
  "author": "Lucas Yang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ycs77/jquery-plugin-bsModal.git"
  },
  "homepage": "https://github.com/ycs77/jquery-plugin-bsModal",
  "bugs": {
    "url": "https://github.com/ycs77/jquery-plugin-bsModal/issues"
  },
  "keywords": [
    "jquery",
    "plugin",
    "jquery-plugin",
    "bootstrap",
    "modal",
    "cropper"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "change-case": "^4.1.1",
    "create-banner": "^1.0.0",
    "cross-env": "^7.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.8.0"
  },
  "peerDependencies": {
    "jquery": "^3.4.1"
  }
}
